{
  "presets": [
    ["es2015", { "modules": false }],
    "react",
    "stage-0"
  ],
  "plugins": [
    [
      "transform-runtime", {
      "helpers": false,
      "polyfill": true,
      "regenerator": true
    }
    ],
    "transform-es3-property-literals",  //ie8
    "transform-es3-member-expression-literals", //ie8
    "transform-export-extensions",
    "transform-decorators-legacy",
    "mobx-deep-action",
    ["import", { "style": "css", "libraryName": "antd-mobile" }]
  ],
  "compact": "true"
}
